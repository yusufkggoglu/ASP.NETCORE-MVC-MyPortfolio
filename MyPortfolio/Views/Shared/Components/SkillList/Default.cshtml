@using EntityLayer.Concrete
@model List<Skill>
<div class="card">
    <div class="card-header">
        <div class="pull-left">
            <h4 class="mt-2">Skills</h4>
            <span class="line"></span>
        </div>
    </div>
    <div class="card-body pb-2">
        @foreach ( var item in Model)
        {
        <h6>@item.SkillName</h6>
        <div class="progress mb-3">
        <div class="progress-bar bg-danger" role="progressbar" style="width: @item.Value%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
        </div>
            
        }
    </div>
</div>